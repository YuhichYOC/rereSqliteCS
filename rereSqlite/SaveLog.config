<?xml version="1.0" encoding="utf-8" ?>
<configuration>

    <configSections>
        <section name="log4net" type=" log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
    </configSections>

    <log4net>

        <appender name="ErrorLog"
                  type="log4net.Appender.RollingFileAppender">
            <file value="Error.log" />
            <appendToFile value="true" />
            <maximumFileSize value="10MB" />
            <maxSizeRollBackups value="5" />
            <layout type="log4net.Layout.PatternLayout">
                <conversionPattern value="%d %-5p - %m%n" />
            </layout>
        </appender>

        <appender name="WarnLog"
                  type="log4net.Appender.RollingFileAppender">
            <file value="Warn.log" />
            <appendToFile value="true" />
            <maximumFileSize value="10MB" />
            <maxSizeRollBackups value="5" />
            <layout type="log4net.Layout.PatternLayout">
                <conversionPattern value="%d %-5p - %m%n" />
            </layout>
        </appender>

        <appender name="InfoLog"
                  type="log4net.Appender.RollingFileAppender">
            <file value="Info.log" />
            <appendToFile value="true" />
            <maximumFileSize value="10MB" />
            <maxSizeRollBackups value="5" />
            <layout type="log4net.Layout.PatternLayout">
                <conversionPattern value="%d %-5p - %m%n" />
            </layout>
        </appender>

        <appender name="DebugLog"
                  type="log4net.Appender.RollingFileAppender">
            <file value="Debug.log" />
            <appendToFile value="true" />
            <maximumFileSize value="10MB" />
            <maxSizeRollBackups value="5" />
            <layout type="log4net.Layout.PatternLayout">
                <conversionPattern value="%d %-5p - %m%n" />
            </layout>
        </appender>

        <appender name="LogOperatorLog"
                  type="log4net.Appender.RollingFileAppender">
            <file value="LogOperator.log" />
            <appendToFile value="true" />
            <maximumFileSize value="10MB" />
            <maxSizeRollBackups value="5" />
            <layout type="log4net.Layout.PatternLayout">
                <conversionPattern value="%d %-5p - %m%n" />
            </layout>
        </appender>

        <appender name="LogFileAppender"
                  type="log4net.Appender.RollingFileAppender">
            <file value="SaveAll.txt" />
            <appendToFile value="true" />
            <maximumFileSize value="10MB" />
            <maxSizeRollBackups value="5" />
            <layout type="log4net.Layout.PatternLayout">
                <conversionPattern value="%d %-5p - %m%n" />
            </layout>
            <filter type="log4net.Filter.LevelRangeFilter">
                <levelMin value="FATAL" />
                <levelMax value="FATAL" />
            </filter>
        </appender>

        <category name="ErrorLog">
            <priority value="ALL" />
            <appender-ref ref="ErrorLog" />
        </category>

        <category name="WarnLog">
            <priority value="ALL" />
            <appender-ref ref="WarnLog" />
        </category>

        <category name="InfoLog">
            <priority value="ALL" />
            <appender-ref ref="InfoLog" />
        </category>

        <category name="DebugLog">
            <priority value="ALL" />
            <appender-ref ref="DebugLog" />
        </category>

        <category name="LogOperatorLog">
            <priority value="ALL" />
            <appender-ref ref="LogOperatorLog" />
        </category>

        <root>
            <priority value="FATAL" />
            <appender-ref ref="LogFileAppender" />
        </root>
    </log4net>
</configuration>